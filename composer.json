{
	"name": "satthi/csv-combine-plugin-for-cakephp",
	"description": "CakePHP CsvCombine",
	"type": "cakephp-plugin",
	"keywords": ["cakephp", "csv"],
	"homepage": "https://github.com/satthi/csv-combine-plugin-for-CakePHP",
	"license": "MIT",
	"authors": [
		{
			"name": "Satoru Hagiwara",
			"role": "Author"
		}
	],
	"support": {
		"source": "https://github.com/satthi/csv-combine-plugin-for-CakePHP"
	},
	"require": {
		"cakephp/cakephp": "~3.0"
	},
	"require-dev": {
		"cakephp/cakephp-codesniffer": "dev-master",
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"CsvCombine\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CsvCombine\\Test\\": "tests",
			"CsvCombine\\Test\\App\\": "tests/test_app/App"
		}
	},
	"suggest": {
	},
	"extra": {
	}
}
